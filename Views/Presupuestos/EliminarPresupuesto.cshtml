@model tl2_tp6_2024_OgaitnaSZ.Models.Presupuesto;
<div class="container">
    <p>Esta seguro que desea eliminar el presupuesto de <strong>@Model.NombreDestinatario</strong>?</p>
    <a class="btn btn-success" asp-area="" asp-controller="Presupuestos" asp-action="Eliminar" asp-route-id="@Model.IdPresupuesto">Aceptar</a>
    <a class="btn btn-danger" asp-area="" asp-controller="Presupuestos" asp-action="ListarPresupuestos">Cancelar</a>
</div>